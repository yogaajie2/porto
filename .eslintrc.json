{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/recommended"
    ],
    "globals": {
      "vue": true
    },
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
      "vue/multi-word-component-names": ["error", {
        "ignores": [
          "about",
          "contact",
          "default",
          "index"
        ]
      }]
    }
}
